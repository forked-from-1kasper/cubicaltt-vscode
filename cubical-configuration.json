{
    "brackets": [
        ["(", ")"],
        ["[", "]"]
    ],
    "surroundingPairs": [
        ["(", ")"],
        ["[", "]"],
        ["<", ">"]
    ],
    "folding": {
        "offSide": true,
        "markers": {
            "start": "^[[:word:]']+",
            "end": "^[[:word:]']+"
        }
    },
    "wordPattern": "^\\(?1:[[:word:]']+\\) [:(]\\|^data \\(?1:[[:word:]']+\\)\\|=\\s-*\\(?1:[[:word:]']\\)\\||\\s-*\\(?1:[[:word:]']\\)"
}